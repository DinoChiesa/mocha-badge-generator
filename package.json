{
    "name": "mocha-badge-generator",
    "version": "0.4.0",
    "description": "Mocha reporter which outputs badge with the number of tests passed and failed.",
    "main": "index.js",
    "scripts": {
        "lint": "eslint .",
        "test": "mocha",
        "test-watch": "mocha -w",
        "coverage": "nyc --reporter=lcov npm run test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ianpogi5/mocha-badge-generator.git"
    },
    "keywords": [
        "mocha",
        "reporter",
        "badge",
        "shield",
        "svg"
    ],
    "author": "Ian Dela Cruz <iandc76@gmail.com>",
    "contributors": [
        "Brett Zamir"
    ],
    "engines": {
        "node": ">=6.0.0"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ianpogi5/mocha-badge-generator/issues"
    },
    "homepage": "https://github.com/ianpogi5/mocha-badge-generator",
    "dependencies": {
        "badge-up": "git+https://github.com/brettz9/badge-up.git#npm-and-testing"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "mocha": "^7.0.0",
        "nyc": "^15.0.0",
        "svg2png": "^4.1.1"
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true,
        "arrowParens": "avoid"
    }
}
